<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.6.xsd">
    <!--
        Added the constraints for entity Surveillant.
    -->
    <changeSet id="20220623153412-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="surveillant"
                                 constraintName="fk_surveillant__user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="surveillant_id"
                                 baseTableName="rel_surveillant__evaluation"
                                 constraintName="fk_rel_surveillant__evaluation__surveillant_id"
                                 referencedColumnNames="id"
                                 referencedTableName="surveillant"/>

        <addForeignKeyConstraint baseColumnNames="evaluation_id"
                                 baseTableName="rel_surveillant__evaluation"
                                 constraintName="fk_rel_surveillant__evaluation__evaluation_id"
                                 referencedColumnNames="id"
                                 referencedTableName="evaluation"/>
    </changeSet>
</databaseChangeLog>
